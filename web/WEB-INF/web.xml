<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Start</servlet-name>
        <servlet-class>co.edu.sena.controller.start.Start</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>redirect</servlet-name>
        <servlet-class>co.edu.sena.controller.redirect.redirect</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Start</servlet-name>
        <url-pattern>/Start</url-pattern>
        <url-pattern>/Roles</url-pattern>
        <url-pattern>/setRolOne</url-pattern>
        <url-pattern>/setMyRol</url-pattern>
        <url-pattern>/RedirectRol</url-pattern>
        <url-pattern>/Logout</url-pattern>
        <url-pattern>/ForgotPass</url-pattern>
        <url-pattern>/ResetPass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>redirect</servlet-name>
        <url-pattern>/Home</url-pattern>
        <url-pattern>/Menu</url-pattern>
        <url-pattern>/Upload</url-pattern>
        <url-pattern>/Search</url-pattern>
        <url-pattern>/SendAgain</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
